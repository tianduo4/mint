<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mint.cms.entity">
	<class name="PopularityItem" table="jc_popularity_item">
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id name="id" type="java.lang.Long" column="popularityitem_id"><generator class="native"/></id>
		<property name="count" column="count" type="integer" not-null="true" length="10"/>
		<many-to-one name="cart" column="cart_id" class="Cart" not-null="true"></many-to-one>
		<many-to-one name="popularityGroup" column="popularity_id" class="PopularityGroup" not-null="false"></many-to-one>
	</class>
</hibernate-mapping>