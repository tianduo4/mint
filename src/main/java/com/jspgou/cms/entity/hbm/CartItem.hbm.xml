<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.jspgou.cms.entity">
    <class name="CartItem" table="jc_shop_cart_item">
        <meta attribute="sync-DAO">false</meta>
        <cache usage="read-write"/>
        <id name="id" type="java.lang.Long" column="cartitem_id">
            <generator class="native"/>
        </id>
        <property name="count" column="count" type="integer" not-null="true" length="10"/>
        <property name="score" column="score" type="integer" not-null="false" length="10"/>
        <many-to-one name="website" column="website_id" class="com.jspgou.core.entity.Website"
                     not-null="true"></many-to-one>
        <many-to-one name="cart" column="cart_id" class="Cart" not-null="true"></many-to-one>
        <many-to-one name="product" column="product_id" class="Product" not-null="true"></many-to-one>
        <many-to-one name="productFash" column="productFash_id" class="ProductFashion" not-null="false"></many-to-one>
        <many-to-one name="popularityGroup" column="popularity_id" class="PopularityGroup"
                     not-null="false"></many-to-one>
    </class>
</hibernate-mapping>