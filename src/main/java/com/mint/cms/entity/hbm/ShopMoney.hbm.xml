<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.mint.cms.entity">
    <class
            name="ShopMoney"
            table="jc_shop_money"
    >
        <meta attribute="sync-DAO">false</meta>
        <id
                name="id"
                type="java.lang.Long"
                column="Id"
        >
            <generator class="native"/>
        </id>
        <property
                name="name"
                column="name"
                type="string"
                not-null="true"
                length="255"
        />
        <property name="money" column="money" type="java.lang.Double" not-null="true" length="20"/>
        <property
                name="status"
                column="status"
                type="boolean"
                not-null="true"
        />
        <property
                name="createTime"
                column="create_time"
                type="timestamp"
                not-null="true"
                length="19"
        />
        <property
                name="remark"
                column="remark"
                type="string"
                not-null="false"
                length="255"
        />
        <many-to-one name="member" column="member_id" class="ShopMember" not-null="false"></many-to-one>

    </class>
</hibernate-mapping>